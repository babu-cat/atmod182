<?xml version="1.0"?>
<extension key="cat.babu.atmod182" type="module">
  <file>atmod182</file>
  <name>M&#xF3;dulo 182 (Agencia Tributaria Espa&#xF1;ola)</name>
  <description>ATMOD182 allows extraction of files to present the 182 form to the Agencia Estatal de Administraci&#xF3;n Tributaria (AEAT) of Spain</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Francesc Bassas i Bullich</author>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/babu-cat/atmod182</url>
    <url desc="Documentation">https://github.com/babu-cat/atmod182</url>
    <url desc="Support">https://github.com/babu-cat/atmod182/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-11-20</releaseDate>
  <version>1.5.11</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <civix>
    <namespace>CRM/Atmod182</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
